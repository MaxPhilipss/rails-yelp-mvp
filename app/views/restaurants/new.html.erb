<h1> CREATE A FUCKING Restaurant!!!  </h1>

<%= simple_form_for [@restaurant] do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.input :category, collection:[ "japanese", "italian", "chinese", "belgian", "french" ]%>

  <%= f.submit "GO GO GO", class: "btn btn-primary" %>
<% end %>
